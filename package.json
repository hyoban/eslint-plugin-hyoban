{
  "name": "eslint-plugin-hyoban",
  "type": "module",
  "version": "0.9.4",
  "packageManager": "pnpm@10.28.2",
  "description": "Hyoban extended ESLint rules.",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-plugin-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-plugin-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-plugin-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.1.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0",
    "bunchee": "^6.9.4",
    "eslint": "^9.39.2",
    "eslint-config-hyoban": "6.0.0-beta.3",
    "eslint-vitest-rule-tester": "^3.0.1",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
